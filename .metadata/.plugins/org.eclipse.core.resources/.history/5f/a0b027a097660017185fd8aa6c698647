package problem2;

public class Ellipse implements ClosedCurve {

	private double side1;
	private double side2;

	public Ellipse(double side1, double side2) {
		this.side1 = side2;
		this.side1 = side2;
	}

	@Override
	public double computePerimeter() {
		// TODO Auto-generated method stub
		return 4 * a * e;
	}

}
