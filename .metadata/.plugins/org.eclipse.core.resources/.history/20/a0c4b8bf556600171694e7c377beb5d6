package classwork;

import java.util.function.Function;

@FunctionalInterface
interface Circle {

	double area(double r);

}

public class Test {

	public static void main(String[] args) {
		
		
		Circle circle = (r) -> Math.PI * Math.pow(r, 2);
		
		
		System.out.println(circle.area(5.5));
		System.out.println(circle.area(7.5));
		
		
		System.out.println("=======================================");
		
		
		Function<Double, Double> circle = (r) -> Math.PI * Math.pow(r, 2);
		

		double x = circle.apply(5.5);
		double y = circle.apply(7.5);
		
		
		System.out.println(x);
		System.out.println(y);
		
		
		
		

	}

}
